{
  "env": {
    "browser": true,
    "jasmine": true
  },
  "rules": {
    "new-cap": 0,
    "no-console": 0,
    "valid-jsdoc": 0,
    "no-sync": 0,
    "no-process-exit": 0,
    "no-underscore-dangle": 0,
    "handle-callback-err": 0,
    "no-ternary": 0,
    "spaced-comment": 0,
    "callback-return": 0,
    "camelcase": 0,
    "no-invalid-this": 0,
    "id-length": 0
  },
  "globals": {
    "angular": true,
    "notify": true,
    "assert": true,
    "inject": true,
    "module": true
  }
}
